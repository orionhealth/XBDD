    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@reduxjs/toolkit": "^1.2.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.3",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/uuid": "^7.0.2",
    "i18next": "^19.3.2",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-xhr-backend": "^3.2.2",
    "immer": "^6.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.0",
    "react-i18next": "^11.3.4",
    "react-redux": "^7.2.0",
    "react-scripts": "^3.4.0",
    "redux": "^4.0.5",
    "rxjs": "^6.5.4",
    "ts-interface-checker": "^0.1.10",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "npm run buildTypeCheckers && run-p start-web start-server",
    "start-web": "react-scripts start",
    "start-server": "nodemon ./dev_proxy/index",
    "build": "react-scripts build",
    "buildTypeCheckersClean": "rm -rf \"src/lib/services/generated\" && mkdir \"src/lib/services/generated\"",
    "buildTypeCheckersGenerate": "ls src/lib/services/*.ts | xargs ts-interface-builder -o \"src/lib/services/generated\" --suffix Types",
    "buildTypeCheckersPrettier": "prettier --write \"src/lib/services/generated/*.ts\"",
    "buildTypeCheckers": "run-s buildTypeCheckersClean buildTypeCheckersGenerate buildTypeCheckersPrettier",
    "lint": "eslint ./src",
    "test": "react-scripts test --env=jsdom",
    "testCI": "react-scripts test --coverage --watchAll=false --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
