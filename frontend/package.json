    "eslint-plugin-react-hooks": "^2.5.1",
    "http-proxy": "^1.17.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.1",
