  "scripts": {
    "start": "npm run buildTypeCheckers && react-scripts start",
    "build": "react-scripts build",
    "buildTypeCheckersClean": "rm -rf \"src/lib/services/generated\" && mkdir \"src/lib/services/generated\"",
    "buildTypeCheckersGenerate": "ls src/lib/services/Fetch*.ts | xargs ts-interface-builder -o \"src/lib/services/generated\" --suffix Types",
    "buildTypeCheckersPrettier": "prettier --write \"src/lib/services/generated/*.ts\"",
    "buildTypeCheckers": "run-s buildTypeCheckersClean buildTypeCheckersGenerate buildTypeCheckersPrettier",
    "lint": "eslint ./src",
    "test": "react-scripts test --env=jsdom",
    "testCI": "react-scripts test --coverage --watchAll=false --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
